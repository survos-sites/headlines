{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "api-platform/core": "^3.2",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.1",
        "halaxa/json-machine": "*",
        "jcobhams/newsapi": "^1.0",
        "knplabs/doctrine-behaviors": "dev-sf7.0 as 2.6.3",
        "league/csv": "^9.16",
        "meilisearch/meilisearch-php": "^1.8",
        "nelmio/cors-bundle": "^2.4",
        "nyholm/psr7": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.28",
        "survos/api-grid-bundle": "*",
        "survos/bootstrap-bundle": "^1.5",
        "survos/command-bundle": "^1.5",
        "survos/pixie-bundle": "^1.5",
        "survos/libre-translate-bundle": "^1.5",
        "survos/simple-datatables-bundle": "^1.5",
        "survos/wiki-bundle": "^1.5",
        "symfony/asset": "^7.0",
        "symfony/asset-mapper": "^7.0",
        "symfony/console": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^7.1",
        "symfony/intl": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "^7.0",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/stimulus-bundle": "^2.17",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/ux-chartjs": "^2.18",
        "symfony/ux-turbo": "^2.17",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/yaml": "^7.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "zenstruck/twig-service-bundle": "^1.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "coverage": "php -d xpcov.enabled=1 vendor/bin/phpunit --coverage-html coverage"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.1"
        }
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^10.1",
        "phpunit/phpunit": "^10.5",
        "survos/maker-bundle": "^1.5",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0"
    },
    "repositories": {
        "jfsenechal_doctrine_behaviors": {
            "type": "vcs",
            "url": "git@github.com:jfsenechal/DoctrineBehaviors.git"
        }
    }
}
